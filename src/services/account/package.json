{
  "name": "@eden/service.account",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "run-s \"clean:*\"",
    "clean:api": "trash api/",
    "clean:buf": "trash buf.lock",
    "clean:go": "trash go.sum",
    "clean:turbo": "trash .turbo/",
    "build": "go build ./cmd/app",
    "format": "run-s \"format:*\"",
    "format:prisma": "prisma format",
    "generate": "run-s \"generate:*\" update:go",
    "generate:pb": "run-s \"generate:pb:*\"",
    "generate:pb:source": "buf export buf.build/eden/account --output ./api",
    "generate:pb:go": "buf generate buf.build/eden/account --include-imports --template=./.buf/buf.go.yaml",
    "generate:pb:micro": "buf generate buf.build/eden/account --template=./.buf/buf.micro.yaml",
    "generate:pb:schema": "zx ./scripts/schema.mjs",
    "generate:templates": "run-s templates:update format:prisma",
    "generate:prisma": "prisma generate",
    "init": "run-s \"init:*\" update generate",
    "init:schema": "mkdir -p ./api/ent/schema",
    "reset": "run-s clean init generate",
    "run": "go run ./cmd/app",
    "templates:update": "hygen service update --name=account",
    "update": "run-s \"update:*\"",
    "update:buf": "buf mod update",
    "update:go": "go mod tidy -compat=1.17"
  },
  "devDependencies": {
    "@apidevtools/swagger-parser": "^10.0.3",
    "@grpc/grpc-js": "^1.5.6",
    "@grpc/proto-loader": "^0.6.9",
    "google-protobuf": "^3.19.4",
    "grpc": "^1.24.11",
    "hygen": "^6.1.0",
    "npm-run-all": "^4.1.5",
    "prisma": "^3.9.2",
    "protobufjs": "^6.11.2",
    "tiny-glob": "^0.2.9",
    "trash-cli": "^5.0.0",
    "zx": "^5.1.0"
  }
}
