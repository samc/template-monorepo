{
  "name": "@taygo/client.contact",
  "version": "0.0.0",
  "private": true,
  "exports": {
    ".": "./src",
    "./*": [
      "./src/*",
      "./src/components/*"
    ]
  },
  "scripts": {
    "barrel": "barrelsby --config=.barrel.json",
    "build": "next build",
    "dev": "next dev --port 3000",
    "format": "run-s \"format:*\"",
    "format:prisma": "prisma format",
    "generate": "run-s \"generate:*\"",
    "generate:pb": "run-s \"generate:pb:*\"",
    "generate:pb:source": "buf export buf.build/taygo/contact --output ./api",
    "generate:pb:graphql": "buf generate buf.build/taygo/contact --include-imports --template=./.buf/buf.graphql.yaml",
    "generate:pb:schema": "zx ./scripts/schema.mjs",
    "generate:template": "hygen client update --name=contact",
    "generate:prisma": "prisma db push; prisma generate",
    "generate:graphql": "graphql-codegen --config codegen.yml",
    "relay": "relay-compiler",
    "start": "next start",
    "test": "jest --watch",
    "update": "ncu --upgrade --packageFile package.json",
    "gen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@emotion/react": "^11.8.1",
