    "@xstate/immer": "^0.3.1",
    "@xstate/inspect": "^0.6.2",
    "immer": "^9.0.12",
    "leva": "^0.9.20",
    "next": "^12.1.0",
    "react": "^17.0.2",
    "react-relay": "^13.1.1",
    "styled-components": "^5.3.3",
    "styled-system": "^5.1.5",
    "tinacms": "^0.66.4",
    "xstate": "^4.30.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.6.2",
