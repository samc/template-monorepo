; Active Chrome Profiles
global Profiles := { Personal: "Default", Work: "GrowThat" }
; Active Windows FancyZones (https://docs.microsoft.com/en-us/windows/powertoys/fancyzones)
global Positions := { Left: "Left", Center: "Center", Right: "Right" }
; Dictionary of Windows executables and their file paths
global Applications := {
  Altair: {
    Name: "Altair",
    Executable: "C:\Shortcuts\Altair.lnk"
  },
  Notepad: {
    Name: "Notepad",
    Executable: "notepad.exe"
  }
}
